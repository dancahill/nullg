	@cd mod_mail;make

_mod_messages:
	@cd mod_messages;make

_mod_notes:
	@cd mod_notes;make

_mod_orders:
	@cd mod_orders;make

_mod_profile:
	@cd mod_profile;make

_mod_searches:
	@cd mod_searches;make

